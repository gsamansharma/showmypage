# Traefik
DOMAIN_MAIN=localhost:3000
DOMAIN_API=localhost:8080
DOMAIN_STUDIO=localhost:3001
TRAEFIK_HOST=subdomain.localhost
TRAEFIK_AUTH_USER=password generated by htpasswd tool

# Database
DB_NAME=your_db_name
DB_URL=jdbc:postgresql://localhost:5432/${DB_NAME}  
DB_USERNAME=your_db_user
DB_PASSWORD=your_db_password

# Backend
JWT_SECRET=your_jwt_secret
SPRING_PROFILES_ACTIVE=dev
FRONTEND_URL={DOMAIN_MAIN}

# OAuth
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# Studio-frontend
BACKEND_API_URL=http://backend:8080
NEXT_PUBLIC_BACKEND_API_URL={DOMAIN_API}
NEXT_PUBLIC_STUDIO_DOMAIN={DOMAIN_STUDIO}
NEXT_PUBLIC_ROOT_DOMAIN={DOMAIN_MAIN}

# AZURE FUNCTIONS
IMAGE_UPLOAD_URL=your_azure_function_url

# Mail
SPRING_MAIL_FROMEMAIL=your_email@example.com
SPRING_MAIL_FROMNAME=Your Name
SPRING_MAIL_HOST=smtp.example.com
SPRING_MAIL_PORT=587
SPRING_MAIL_USERNAME=your_smtp_username
SPRING_MAIL_PASSWORD=your_smtp_password

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# Traefik / Let's Encrypt / Cloudflare
LETSENCRYPT_EMAIL=your_letsencrypt_email
CFI_API_EMAIL=your_cloudflare_email
CF_API_KEY=your_cloudflare_api_key